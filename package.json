{
    "name": "gitlanding",
    "version": "1.0.0",
    "description": "A module that generates a landing page for your projects",
    "repository": {
        "type": "git",
        "url": "git://github.com/thieryw/gitlanding.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc && yarn copyfiles",
        "copyfiles": "copyfiles -u 1 src/**/*.svg dist/",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "storybook": "start-storybook -p 5005",
        "build-storybook": "build-storybook",
        "yarn_link": "ts-node --skip-project scripts/yarn_link.ts",
        "link-in-app": "ts-node --skip-project scripts/link-in-external-project.ts",
        "start_vanilla": "yarn yarn_link && cd src/test/vanilla && yarn start"
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/thieryw",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/thieryw/gitlanding",
    "peerDependencies": {
        "@mui/material": "^5.9.3",
        "@types/react": "^16.8.0 || ^17.0.0 || ^18.0.0",
        "onyxia-ui": "^1.0.0",
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        }
    },
    "dependencies": {
        "@mui/icons-material": "^5.5.0",
        "embla-carousel-react": "^7.0.3",
        "framer-motion": "^4.1.17",
        "memoizee": "^0.4.15",
        "powerhooks": "^1.0.0",
        "react-github-btn": "^1.2.0",
        "react-markdown": "^5.0.3",
        "react-svg": "^15.0.4",
        "react-syntax-highlighter": "^15.5.0",
        "tsafe": "^1.6.4",
        "tss-react": "^4.9.0",
        "typeface-work-sans": "^1.1.13"
    },
    "devDependencies": {
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.3.0",
        "@mui/material": "^5.10.3",
        "@emotion/css": "^11.10.0",
        "@storybook/addon-actions": "^6.5.9",
        "@storybook/addon-docs": "^6.5.9",
        "@storybook/addon-essentials": "^6.5.9",
        "@storybook/addon-links": "^6.5.9",
        "@storybook/preset-create-react-app": "^4.1.2",
        "@storybook/builder-webpack5": "^6.5.9",
        "@storybook/manager-webpack5": "^6.5.9",
        "@storybook/node-logger": "^6.5.9",
        "@storybook/react": "^6.5.9",
        "@types/memoizee": "0.4.7",
        "@types/node": "^18.15.11",
        "@types/react": "^18.0.14",
        "@types/react-dom": "18.0.6",
        "@types/react-syntax-highlighter": "^15.5.2",
        "copyfiles": "^2.4.1",
        "evt": "^2.5.2",
        "husky": "^4.3.0",
        "lint-staged": "^10.5.4",
        "onyxia-ui": "^1.0.0",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "storybook-dark-mode": "^1.1.0",
        "ts-node": "^10.4.0",
        "typescript": "^4.7.4",
        "webpack": "^5.73.0"
    }
}
