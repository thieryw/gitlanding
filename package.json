{
    "name": "gitlanding",
    "version": "2.0.15",
    "description": "A module that generates a landing page for your projects",
    "repository": {
        "type": "git",
        "url": "git://github.com/thieryw/gitlanding.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsx scripts/build.ts",
        "link-in-demo": "tsx scripts/link-in-demo.ts",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different"
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/thieryw",
    "license": "MIT",
    "files": [
        "src/",
        "!src/test/",
        "dist/",
        "!dist/test/",
        "!dist/tsconfig.tsbuildinfo"
    ],
    "keywords": [],
    "homepage": "https://github.com/thieryw/gitlanding",
    "peerDependencies": {
        "onyxia-ui": "^5.4.2",
        "@mui/material": "^5.0.0",
        "@emotion/react": "^11.0.0"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        }
    },
    "dependencies": {
        "@mui/icons-material": "^5.5.0",
        "embla-carousel-react": "^7.0.3",
        "framer-motion": "^4.1.17",
        "powerhooks": "^1.0.17",
        "react-github-btn": "^1.2.0",
        "react-syntax-highlighter": "^15.5.0",
        "tsafe": "^1.8.1",
        "tss-react": "^4.9.13"
    },
    "devDependencies": {
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@mui/material": "^5.10.3",
        "@types/node": "^22.5.5",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@types/react-syntax-highlighter": "^15.5.2",
        "evt": "^2.5.7",
        "husky": "^4.3.0",
        "lint-staged": "^10.5.4",
        "onyxia-ui": "^5.4.2",
        "prettier": "^2.7.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "storybook-dark-mode": "^1.1.0",
        "typescript": "^5.6.2",
        "tsx": "^4.19.1",
        "chokidar": "^3.6.0"
    }
}
